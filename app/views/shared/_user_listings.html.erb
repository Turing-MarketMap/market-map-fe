<% if @current_user.present? %>
  <table class="table table-hover">
    <thead>
      <tr>
        <th scope="col">Price</th>
        <th scope="col">Year</th>
        <th scope="col">Make</th>
        <th scope="col">Model</th>
      </tr>
    </thead>
    <% if @user_listings %>

    <tbody>
  <% @user_listings.each do |listing| %>
      <tr>
        <th scope="row">1</th>
        <td>listing.price</td>
      </tr>
      <tr>
        <th scope="row">2</th>
        <td>listing.year</td>
      </tr>
      <tr>
        <th scope="row">3</th>
        <td>listing.make</td>
      </tr>
      <tr>
        <th scope="row">4</th>
        <td>listing.model</td>
      </tr>
    <% end %>
  <% end %>
  </tbody>
  </table>
<% end %>
